{
  "expo": {
    "name": "Solimo",
    "slug": "solimo",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-app-1024.svg",
    "userInterfaceStyle": "light",
    "description": "Solimo - A delightful micro-learning app for kids 3-8 with one engaging activity per day. Voice-accessible, beautifully animated, and designed for parents who want screen time to actually matter.",
    "splash": {
      "image": "./assets/icon-app-1024.svg",
      "resizeMode": "contain",
      "backgroundColor": "#FFF9E6"
    },
    "ios": {
      "bundleIdentifier": "com.solimo.app",
      "supportsTabletMode": true,
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-app-1024.svg",
        "backgroundColor": "#FFF9E6"
      },
      "package": "com.lumini.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "scheme": "lumini",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-audio"
    ],
    "owner": "fuentes-studio",
    "extra": {
      "eas": {
        "projectId": "436debcc-3238-4023-8b9f-c76c9ab1cdfb"
      }
    }
  }
}
